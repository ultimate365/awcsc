+import { apiUrls } from "./api.url.js";
+
+function postAPI(url, body) {
+  return fetch(url, {
+    method: "POST",
+    body: JSON.stringify(body),
+    headers: {
+      "Content-Type": "application/json"
+    }
+  }).then(response => response.json());
+}
+
+function getAPI(url) {
+  return fetch(url, {
+    method: "GET",
+    headers: {
+      "Content-Type": "application/json"
+    }
+  }).then(response => response.json());
+}
+
+export class ApiService {
+  static async sendOTP(body) {
+    return postAPI(apiUrls.sendOTP, body);
+  }
+
+  static async verifyOTP(body) {
+    return postAPI(apiUrls.verifyOTP, body);
+  }
+
+  static async retryOTP(body) {
+    return postAPI(apiUrls.retryOTP, body);
+  }
+
+  static async getWidgetProcess(widgetId, tokenAuth) {
+    return getAPI(
+      apiUrls.getWidgetProcess
+        .replace(":widgetId", widgetId)
+        .replace(":tokenAuth", tokenAuth)
+    );
+  }
+}
