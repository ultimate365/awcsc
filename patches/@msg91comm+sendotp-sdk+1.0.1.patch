+import { ApiService } from "./API/api.service.js";
+
+let widgetId = "";
+let tokenAuth = "";
+
+export class OTPWidget {
+    static checkInitialization() {
+        if (widgetId === "" || tokenAuth === "") {
+            console.error("Widget not initialized. Call initializeWidget before using any method.");
+            return false;
+        }
+        return true;
+    }
+
+    static async initializeWidget(widgetid, tokenauth) {
+        widgetId = widgetid;
+        tokenAuth = tokenauth;
+    }
+
+    static async sendOTP(body) {
+        if (!this.checkInitialization()) return;
+
+        const payload = {
+            widgetId: widgetId,
+            tokenAuth: tokenAuth,
+            ...body
+        };
+
+        try {
+            return await ApiService.sendOTP(payload);
+        } catch (error) {
+            console.error("Error sending OTP:", error);
+            throw error;
+        }
+    }
+
+    static async verifyOTP(body) {
+        if (!this.checkInitialization()) return;
+
+        const payload = {
+            widgetId: widgetId,
+            tokenAuth: tokenAuth,
+            ...body
+        };
+
+        try {
+            return await ApiService.verifyOTP(payload);
+        } catch (error) {
+            console.error("Error verifying OTP:", error);
+            throw error;
+        }
+    }
+
+    static async retryOTP(body) {
+        if (!this.checkInitialization()) return;
+
+        const payload = {
+            widgetId: widgetId,
+            tokenAuth: tokenAuth,
+            ...body
+        };
+
+        try {
+            return await ApiService.retryOTP(payload);
+        } catch (error) {
+            console.error("Error sending retry OTP:", error);
+            throw error;
+        }
+    }
+
+    static async getWidgetProcess() {
+        if (!this.checkInitialization()) return;
+
+        try {
+            return await ApiService.getWidgetProcess(widgetId, tokenAuth);
+        } catch (error) {
+            console.error("Error getting widget process:", error);
+            throw error;
+        }
+    }
+}
